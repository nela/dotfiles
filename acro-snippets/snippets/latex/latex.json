{
  "DeclareAcronym": {
    "prefix": "deca",
    "body": [
      "\\DeclareAcronym{${1:acronym reference}}{",
      "\tshort = ${2:acronym},",
      "\tlong = ${0:full name}",
      "}"
    ],
    "description": "Declare acronym"
  },
  "Acronym": {
